@model ESS.Models.EmailTemplate

@{
    ViewBag.Title = "DecryptPassword";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>DecryptPassword</h2>
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Decrypt Password</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>

    <form role="form" action="~/Home/GetDecrypt" method="post">
        <div class="form-group">
            <label>Input Text Code</label>
            <input class="form-control" name="inputtextcode">

        </div>
        <button type="submit" class="btn btn-default">Submit</button>
        @*<button type="reset" class="btn btn-default" onclick="window.history.go(-1); return false;">Cancel</button>*@
        <div class="form-group">
            <label>Result</label>
            <input class="form-control" name="showcode" value="@ViewData["showcode"]">
        </div>


    </form>
</div>
