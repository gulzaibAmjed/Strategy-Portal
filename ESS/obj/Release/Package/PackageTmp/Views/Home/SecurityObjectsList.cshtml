@{
    ViewBag.Title = "EmailTemplate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">@ViewBag.tablecaption Objects</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <div class="row">
        <div class="col-lg-12">

            <table class="table table-striped table-bordered table-hover dataTable no-footer" role="grid" cellspacing="0" style="width:100%">
                <thead>

                    <tr role="row">
                        <th role="columnheader">
                            Table Name
                        </th>


                        <th role="columnheader">
                            Actions
                        </th>

                    </tr>
                    @*</thead>
                    <tbody id="helpRowBody">
                        @Html.Raw(ViewData["MattersTable"]))
                    </tbody>
                </table>*@
                </thead>
                <tbody>
                    @foreach (var x in @Model)
                    {
                        <tr>
                            <td>
                                @x[0]
                            </td>


                            <td>
                                <a href="@Url.Action(controllerName: "Home", actionName: "SecurityData", routeValues: new { name = @x[0] })" class="btn btn-primary btn-circle" role="button"><i class="fa fa-list"></i></a>


                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
