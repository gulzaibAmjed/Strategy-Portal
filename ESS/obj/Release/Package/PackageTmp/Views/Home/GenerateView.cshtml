@model List<ESS.Models.HTMLTemplate>

@{
    ViewBag.Title = "Dynamic View";


}
<script language="javascript" type="text/javascript">
    $(document).ready(function () {
        var t = $("#form1");
        t[0][0]["disabled"] = true;
    });
    </script>

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Edit Table Data</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <form role="form" method="post" action="/Home/UpdateFieldValue" id="form1">

                <div class="form-group">
                    @foreach (var d in Model)
                    {
                        @Html.Label(@d.FIELD_LABEL)
                        @Html.TextBox(@d.FIELD_LABEL, @d.FIELD_VALUE, null, new { @class = "form-control" })
                        

                    }
                </div>
                <button type="submit" class="btn btn-default">Update</button>
                <button type="submit" class="btn btn-default" onclick="window.history.go(-1); return false;">Cancel</button>

            </form>
        </div>



